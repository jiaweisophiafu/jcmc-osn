@article{ ISI:000386301700006,
Author = {Bosch, Tanja},
Title = {{Twitter activism and youth in South Africa: the case of \#RhodesMustFall}},
Journal = {{INFORMATION COMMUNICATION \& SOCIETY}},
Year = {{2017}},
Volume = {{20}},
Number = {{2}},
Pages = {{221-232}},
Abstract = {{This article uses the South African student-led campaign known as Rhodes
   Must Fall, commonly referred to simply as \#RMF, to explore youth
   activism and counter-memory via social networking site Twitter. The RMF
   campaign took place at the University of Cape Town and comprised
   student-led protests, which campaigned to remove the statue of British
   colonialist Cecil John Rhodes, as activists argued that it promoted
   institutionalized racism and promoted a culture of exclusion
   particularly for black students. Through a qualitative content analysis
   of tweets and a network analysis using NodeXL, this article argues that
   despite the digital divide in South Africa, and limited access to the
   internet by the majority of citizens, Twitter was central to youth
   participation during the RMF campaign, reflecting the politics and
   practices of counter-memory but also setting mainstream news agendas and
   shaping the public debate. The article further argues that the \#RMF
   campaign can be seen a collective project of resistance to normative
   memory production. The analysis demonstrates how social media
   discussions should not be viewed as detached from more traditional media
   platforms, particularly, as in this case, they can set mainstream news
   agendas. Moreover, the article argues that youth are increasingly using
   social networking sites to develop a new biography of citizenship which
   is characterized by more individualized forms of activism. In the
   present case, Twitter affords youth an opportunity to participate in
   political discussions, as well as discussions of broader socio-political
   issues of relevance in contemporary South African society, reflecting a
   form of subactivism.}},
Publisher = {{ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD}},
Address = {{2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Bosch, T (Reprint Author), Univ Cape Town, Ctr Film \& Media Studies, Cape Town, South Africa.
   Bosch, Tanja, Univ Cape Town, Ctr Film \& Media Studies, Cape Town, South Africa.}},
