@article{ ISI:000327246900005,
Author = {Soderberg, Johan},
Title = {{Determining social change: The role of technological determinism in the
   collective action framing of hackers}},
Journal = {{NEW MEDIA \& SOCIETY}},
Year = {{2013}},
Volume = {{15}},
Number = {{8}},
Pages = {{1277-1293}},
Month = {{DEC}},
Abstract = {{This article takes the political engagement of hackers as a prism for
   examining the relations between technological determinist thinking and
   collective action. The concept collective action framing' is borrowed
   from social movement theory to describe how hackers have appropriated
   notions of a post-industrial, information society in their struggles
   against intellectual property laws and state censorship. Hackers have
   reintroduced an element of conflict and antagonism into otherwise
   politically innocuous visions of post-industrialism. This residual of
   antagonism can be traced back to the roots of the post-industrial myth
   in Marxist, historical materialist theory. By exploring these origins,
   the article proceeds to compare the hopes invested by hackers in the
   emancipatory force of information technology with the earlier beliefs of
   labour movements that the forces of history were on their side. Building
   on this comparison it is argued that technological determinism does not
   always lead to political resignation, but can also serve as a foundation
   for collective action.}},
