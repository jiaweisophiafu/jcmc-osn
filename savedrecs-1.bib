@article{ ISI:000344245500009,
Author = {Light, Ben and Cassidy, Elija},
Title = {{Strategies for the suspension and prevention of connection: Rendering
   disconnection as socioeconomic lubricant with Facebook}},
Journal = {{NEW MEDIA \& SOCIETY}},
Year = {{2014}},
Volume = {{16}},
Number = {{7, SI}},
Pages = {{1169-1184}},
Month = {{NOV}},
Abstract = {{This article attends to the idea of disconnection as a way of theorising
   people's lived experience of social networking sites. Enrolling and
   extending a disconnective practice lens, we suggest that the
   disconnective strategies of suspension and prevention are operational
   necessities for those we might see as the users and owners of sites such
   as Facebook. Indeed, our work demonstrates that disconnection in these
   contexts need not be associated only with modes of resistance and
   departure, but can also act as socioeconomic lubricant.}},
