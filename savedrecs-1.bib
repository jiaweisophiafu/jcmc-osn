@article{ ISI:000403190100009,
Author = {Stevens, Robin and Gilliard-Matthews, Stacia and Dunaev, Jamie and
   Woods, Marcus K. and Brawner, Bridgette M.},
Title = {{The digital hood: Social media use among youth in disadvantaged
   neighborhoods}},
Journal = {{NEW MEDIA \& SOCIETY}},
Year = {{2017}},
Volume = {{19}},
Number = {{6}},
Pages = {{950-967}},
Month = {{JUN}},
Abstract = {{This study examines the role of social media in the lives of youth
   living in disadvantaged neighborhoods. Feminist Standpoint Theory, which
   privileges the voices of marginalized communities in understanding
   social phenomena, suggests that youth at the margins have specific
   knowledge that helps us understand social media more broadly. We
   conducted semi-structured interviews with 30 females and 30 males aged
   13-24years about their social worlds and neighborhoods, both online and
   offline. The findings reveal a dynamic and somewhat concerning interplay
   between the geographic neighborhood and the digital neighborhood,
   whereby negative social interactions in the geographic neighborhood are
   reproduced and amplified on social media.}},
