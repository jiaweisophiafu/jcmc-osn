@article{ ISI:000329526000004,
Author = {Campbell, Heidi A.},
Title = {{Religion and the Internet: A microcosm for studying Internet trends and
   implications}},
Journal = {{NEW MEDIA \& SOCIETY}},
Year = {{2013}},
Volume = {{15}},
Number = {{5, SI}},
Pages = {{680-694}},
Month = {{AUG}},
Abstract = {{This article argues that paying close attention to key findings within
   the study of religion and the Internet, a subfield of Internet Studies,
   can enhance our understanding and discussion of the larger social and
   cultural shifts at work within networked society. Through a critical
   overview of research on religion online, five central research areas
   emerge related to social practices, online-offline connections,
   community, identity, and authority online. It is also argued that
   observations about these themes not only point to specific trends within
   religious practice online, but also mirror concerns and findings within
   other areas of Internet Studies. Thus, studying religion on the Internet
   provides an important microcosm for investigating Internet Studies'
   contribution in a wide range of contexts in our contemporary social
   world.}},
