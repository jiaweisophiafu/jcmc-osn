@article{ ISI:000322692600010,
Author = {De Stefano, Domenico and Fuccella, Vittorio and Vitale, Maria Prosperina
   and Zaccarin, Susanna},
Title = {{The use of different data sources in the analysis of co-authorship
   networks and scientific performance}},
Journal = {{SOCIAL NETWORKS}},
Year = {{2013}},
Volume = {{35}},
Number = {{3}},
Pages = {{370-381}},
Month = {{JUL}},
Abstract = {{Scientific collaboration is usually derived from archival co-authorship
   data. Several data sources may be examined, but they all have advantages
   and disadvantages, especially when a specific discipline or community is
   of interest. The aim of this paper is to explore the effect of the use
   of three data sources Web of Science, Current Index to Statistics and
   nationally funded research projects - on the analysis of co-authorship
   networks among Italian academic statisticians. Results provide evidence
   of our hypotheses on distinct collaboration patterns among
   statisticians, as well as distinct effects of scientist network
   positions on scientific performance, by both Statistics subfield and
   data source. (c) 2013 Elsevier B.V. All rights reserved.}},
