@article{ ISI:000304470300005,
Author = {Hargittai, Eszter and Litt, Eden},
Title = {{BECOMING A TWEEP How prior online experiences influence Twitter use}},
Journal = {{INFORMATION COMMUNICATION \& SOCIETY}},
Year = {{2012}},
Volume = {{15}},
Number = {{5, SI}},
Pages = {{680-702}},
Abstract = {{Despite much excitement about the microblogging platform Twitter, little
   is known about predictors of its adoption and how its uses relate to
   other online activities in particular. Using a unique longitudinal data
   set from 2009 to 2010 surveying over 500 diverse young American adults
   about their online experiences, we look at how adoption of Twitter
   relates to prior engagement in other types of online activities. Our
   findings suggest that online skills as well as prior consumption and
   production activities especially in the domain of entertainment news are
   significant predictors of subsequent Twitter use. Our results caution
   about the potential biases that may result from studies that sample on
   Twitter users excluding other populations.}},
