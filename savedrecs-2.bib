@article{ ISI:000299819900001,
Author = {Baasanjav, Undrahbuyan},
Title = {{WEB-USE PATTERNS FOR CIVIC DISCOURSE The case of Mongolian organizations}},
Journal = {{INFORMATION COMMUNICATION \& SOCIETY}},
Year = {{2011}},
Volume = {{14}},
Number = {{5}},
Pages = {{591-618}},
Abstract = {{This research examines communicative and interactive features of
   Mongolian-language civic-discourse web sites by applying the media
   typology models proposed by Bordewijk and Van Kaam and Van Dijk. Based
   on the results of the content analysis of Mongolian governmental and
   civil society institutions, this research provides (1) empirical
   evidence useful for understanding the role of the Internet in social and
   political practices; and (2) a plausible model for assessing interactive
   characteristics of civic-discourse web sites. The results of the study
   support the view that the Internet is mostly shaped by existing
   political and social settings and routines. This does not discount the
   fact that newly established structures such as diaspora networks, new
   media organizations and interest groups are becoming more resourceful in
   using the interactive features of the Internet to expand the civic
   discourse in the Mongolian language.}},
