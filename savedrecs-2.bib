@article{ ISI:000298122200008,
Author = {Brashears, Matthew E.},
Title = {{Small networks and high isolation? A reexamination of American
   discussion networks}},
Journal = {{SOCIAL NETWORKS}},
Year = {{2011}},
Volume = {{33}},
Number = {{4}},
Pages = {{331-341}},
Month = {{OCT}},
Abstract = {{Recent findings from the General Social Survey suggest that discussion
   networks have shrunk, and social isolation has increased, since 1985.
   These data have been threatened by possible artifacts, however, and may
   be unreliable. This paper reports on a nationally representative
   experiment that estimates discussion network size and evaluates several
   proposed explanations for the shrinking networks finding. It finds that
   modern discussion networks have decreased in size, but that social
   isolation has not become more prevalent. It also disconfirms several
   explanations for shrinking networks, and validates a method for
   increasing the number of names provided in response to name generators.
   (C) 2011 Elsevier B.V. All rights reserved.}},
