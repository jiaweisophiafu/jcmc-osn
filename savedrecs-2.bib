@article{ ISI:000288981900005,
Author = {Earl, Jennifer},
Title = {{THE DYNAMICS OF PROTEST-RELATED DIFFUSION ON THE WEB}},
Journal = {{INFORMATION COMMUNICATION \& SOCIETY}},
Year = {{2010}},
Volume = {{13}},
Number = {{2}},
Pages = {{209-225}},
Abstract = {{Drawing on several larger literatures on diffusion processes, including
   literatures on the diffusion of innovations, disease spread, and the
   diffusion of information, this paper examines the classes of diffusion
   processes that may be relevant to understanding online protest and
   social movements. The author also argues that one commonly studied type
   of online, protest-related diffusion process - the online diffusion of
   information - has only minor theoretical implications for social
   movement theory. Two other diffusion processes - the diffusion of
   online, protest-related innovations and the diffusion of protest in its
   many forms as a problem-solving heuristic to new populations - are
   likely to qualitatively alter other (non-diffusion) social movement
   processes, creating important second-order, theoretical effects from
   these types of diffusion.}},
Publisher = {{ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD}},
