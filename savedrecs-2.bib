@article{ ISI:000255464200002,
Author = {Shumate, Michelle and Dewitt, Lori},
Title = {{The north/south divide in NGO hyperlink networks}},
Journal = {{JOURNAL OF COMPUTER-MEDIATED COMMUNICATION}},
Year = {{2008}},
Volume = {{13}},
Number = {{2}},
Pages = {{405-428}},
Month = {{JAN}},
Note = {{56th Annual Conference of the International-Communication-Association,
   Dresden, GERMANY, JUN 19-23, 2006}},
Organization = {{Int Commun Assoc}},
Abstract = {{Globalization and communication scholars have argued that technology is
   transforming the ``third sector'', the set of organizations that are
   not-for-profit and non-governmental (NGOs). This research examines the
   local and global linkages among a hyperlink network of 248 HIV/AIDS
   NGOs. This research examines the north/south NGO divide in the context
   of these new technology-based associations. Results suggest that the
   north/south divide is as great a concern in the virtual world as it is
   in the physical one. This research implies that globalization theorists'
   argument that communication technologies break down economic and
   geographic barriers may be overstating the empirical reality.}},
