@article{ ISI:000280765800007,
Author = {Broadfoot, Kirsten J. and Munshi, Debashish and Nelson-Marsh, Natalie},
Title = {{COMMUNEcation: a rhizomatic tale of participatory technology,
   postcoloniality and professional community}},
Journal = {{NEW MEDIA \& SOCIETY}},
Year = {{2010}},
Volume = {{12}},
Number = {{5}},
Pages = {{797-812}},
Month = {{AUG}},
Abstract = {{This article explores the authors' experiences in creating and
   participating with(in) a virtual conference organized as an experimental
   virtual network. These experiences demonstrate how physically co-located
   and virtual conferencing practices acting in tandem provide a greater
   opportunity for the inclusion of both diverse perspectives and
   participants in professional communities. Using insights from
   postcolonial theory, the article examines how the architecture of
   participation found in the technologies of Web 2.0 accentuates the
   potential for reclaiming some diversity of perspective and
   participation, facilitating a form of molecular community through
   conferencing practices. Finally, it provides theoretical and empirical
   insights and reflections on the social dynamics of conferencing in both
   online and offline spaces to demonstrate how online conferencing can
   expand the directions taken in pursuit of new collective knowledge.}},
