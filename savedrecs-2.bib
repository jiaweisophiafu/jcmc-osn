@article{ ISI:000240890700007,
Author = {Sozanski, Tadeusz},
Title = {{On the core of characteristic function games associated with exchange
   networks}},
Journal = {{SOCIAL NETWORKS}},
Year = {{2006}},
Volume = {{28}},
