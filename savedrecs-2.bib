@article{ ISI:000303561300003,
Author = {Halavais, Alexander M. C.},
Title = {{A GENEALOGY OF BADGES Inherited meaning and monstrous moral hybrids}},
Journal = {{INFORMATION COMMUNICATION \& SOCIETY}},
Year = {{2012}},
Volume = {{15}},
Number = {{3, SI}},
Pages = {{354-373}},
Abstract = {{The use of badges on the web, particularly on community sites, has
   become very popular, and these badges are becoming both more easily
   carried from one site to another and more valuable in the process. But
   badges are not new; the metaphor of the online badge draws on centuries
   of use in the offline world. And the use of badges online has the
   potential of bringing with it the echo of these earlier uses and the
   values that they were imbued with. This article explores online badges,
   drawing on their history and the ethical framework presented by Jane
   Jacobs in `Systems of Survival' to suggest some ways of ensuring that
   badges are used effectively online.}},
