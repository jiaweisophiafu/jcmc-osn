@article{ ISI:000174748200006,
Author = {Kunkel, D and Farinola, WJM and Farrar, K and Donnerstein, E and Biely,
   E and Zwarun, L},
Title = {{Deciphering the V-chip: An examination of the television industry's
   program rating judgments}},
Journal = {{JOURNAL OF COMMUNICATION}},
Year = {{2002}},
Volume = {{52}},
Number = {{1}},
Pages = {{112-138}},
Month = {{MAR}},
Abstract = {{This study, investigated the validity of the television industry's
   labeling of sensitive program content following the advent of the V-chip
   television ratings system. The analysis is based on a large program
   sample gathered from 10 of the most frequently, viewed channels
   including broadcast network, independent broadcast, basic cable, and
   premium cable channels. Programs were examined for the nature and extent
   of portrayals of violence, sexual behavior and dialogue, and adult
   language. The content-based findings were then compared tu the ratings
   applied to the program to evaluate the accuracy, of the rating
   judgments. The stud found that the age-based ratings (TV-G, TV-PG,
   TV-14, and TV-MA) reasonably, reflect the content of programs, but that
   content descriptors (V, 5, D, and L) are not being used on the vast
   majority of programs containing violence, sexual behavior or dialogue,
   and adult language. The study, also shows that children's programs
   contain a significant amount of violence, most of which is not
   identified by, the applicable FV(fantasy violence) content descriptor.
   The findings suggest there are substantial limitations in the ability of
   the V-chip technology to effectively limit children's exposure to most
   forms of sensitive content.}},
