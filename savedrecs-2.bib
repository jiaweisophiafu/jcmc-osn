@article{ ISI:000237100900014,
Author = {Matei, SA},
Title = {{From counterculture to cyberculture: Virtual community discourse and the
   dilemma of modernity}},
Journal = {{JOURNAL OF COMPUTER-MEDIATED COMMUNICATION}},
Year = {{2005}},
Volume = {{10}},
Number = {{3}},
Month = {{APR}},
Abstract = {{Virtual communities are discussed as expressions of the modern tension
   between individuality and Community, emphasizing the role that
   counterculture and its values played in shaping the virtual community
   project. This article analyzes postings to the WELL conferences and the
   online groups that served as incubators and testing ground for the term
   ``virtual community,{''} revealing how this concept was Culturally
   shaped by the countercultural ideals of WELL users and how the tension
   between individualism and communication ideals was dealt with. The
   overarching conclusion is that virtual communities act both as solvent
   and glue in modern society, being similar to the ``small group{''}
   movement.}},
