@article{ ISI:000329526000002,
Author = {Peng, Tai-Quan and Zhang, Lun and Zhong, Zhi-Jin and Zhu, Jonathan J. H.},
Title = {{Mapping the landscape of Internet Studies: Text mining of social science
   journal articles 2000-2009}},
Journal = {{NEW MEDIA \& SOCIETY}},
