@article{ ISI:000329776500005,
Author = {Khan, Gohar Feroz and Yoon, Ho Young and Park, Han Woo},
Title = {{Social media communication strategies of government agencies: Twitter
   use in Korea and the USA}},
Journal = {{ASIAN JOURNAL OF COMMUNICATION}},
Year = {{2014}},
Volume = {{24}},
Number = {{1, SI}},
Pages = {{60-78}},
Month = {{JAN 2}},
Abstract = {{This study examines Twitter use by the central government in Korea and
   the federal government in the USA by employing the webometric technique
   to extract their Twitter activity (basic Twitter statistics such as the
   numbers of followers, followings, and Tweets) and the social network
   analysis technique to map the relationship between their Twitter
   accounts and the direction of outlinks in their Tweets. The results of
   the initial analysis indicate some differences in Twitter strategies
   between the two governments. For example, Korean ministries were well
   connected through a dense network, engaged in collective cooperation,
   and retweeted common content to reinforce their collective agendas
   regardless of their main administrative functions, whereas US government
   departments were less collective and more individualistic and retweeted
   those messages that specifically fit the purpose of each department. In
   addition, the results for outlinks indicate that US government
   departments preferred private sources of information, whereas Korean
   ministries, government sources.}},
