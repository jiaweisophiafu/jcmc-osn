Author = {Goetz, Georg},
Title = {{Competition, regulation, and broadband access to the internet}},
Journal = {{TELECOMMUNICATIONS POLICY}},
Year = {{2013}},
Volume = {{37}},
Number = {{11}},
Pages = {{1095-1109}},
Month = {{DEC}},
Abstract = {{This paper reexamines the effect of the regulatory regime on both
   penetration and coverage of broadband access to the internet. The
   framework allows for an evaluation of policy initiatives, guidelines and
   measures to bridge the digital divide and to promote investment. A
   welfare analysis compares service-based with facilities-based
   competition and asks whether and how high-speed access to the internet
   should be subsidized. Using an approach similar to Valletti, Barros, and
   Hoernig (2002), the paper highlights the importance of population
   density for whether firms invest to provide internet access. The
   analysis reveals a trade-off between coverage and penetration. (C) 2013
   Elsevier Ltd. All rights reserved.}},
