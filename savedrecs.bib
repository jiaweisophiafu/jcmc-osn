@article{ ISI:000222510700002,
Author = {Matei, S},
Title = {{The impact of state-level social capital on the emergence of virtual
   communities}},
Journal = {{JOURNAL OF BROADCASTING \& ELECTRONIC MEDIA}},
Year = {{2004}},
Volume = {{48}},
Number = {{1}},
Pages = {{23-40}},
Month = {{MAR}},
Abstract = {{The paper analyzes the 48 contiguous states of the Union and their
   ability to create and maintain online communities (Yahoo! groups).
   Multiple regression analysis indicates that the number of online groups
   and overall amount of online activity increase with amount of social
   capital. Also, ethnic homogeneity positively influences the number of
   online groups, while population density and number of IT workers are
   positively associated with level of online activity. In broad terms, the
   analyses support the idea that the Internet strengthens offline
