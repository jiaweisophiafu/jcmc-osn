@article{ ISI:000366337400001,
Author = {Bentivegna, Sara and Marchetti, Rita},
Title = {{Live tweeting a political debate: The case of the ``Italia bene comune'}},
Journal = {{EUROPEAN JOURNAL OF COMMUNICATION}},
Year = {{2015}},
Volume = {{30}},
Number = {{6}},
Pages = {{631-647}},
Month = {{DEC}},
Abstract = {{The practice of live tweeting while following a television programme is
   quickly becoming a widespread phenomenon. However, in Italy, commenting
   on a political debate via social media is a novelty. This article
   presents the first exemplary case study in an Italian context, the
   Italia bene comune' debate, hosted by SkyTg24 on 12 November 2012. This
   debate focused on elections to designate a candidate to lead the
   government from the centre-left coalition. The findings indicate the new
   hybridisation of media: television's centrality is confirmed regarding
   its domination in the public space in which political discussion occurs,
   and the web demonstrates its own capacity for an active role in an event
   central to Italian politics. The new element was the active
   participation of a vocal minority who commented online as the media
   event unfolded live on television.}},
